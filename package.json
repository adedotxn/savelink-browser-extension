{
  "name": "savelink-chrome-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev && npm-watch",
    "lint:build": "mv out/_next out/assets && sed -i 's/\\\\/_next/\\\\/assets/g' out/**.html",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint"
  },
  "watch": {
    "build": {
      "patterns": [
        "styles/",
        "pages/",
        "public/",
        "helpers/",
        "next.config.js"
      ],
      "ignore": [
        "out",
        "node_modules",
        ".next",
        ".vscode"
      ],
      "extensions": [
        "js",
        "json"
      ]
    }
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.208",
    "@types/firefox-webext-browser": "^94.0.1"
  }
}
